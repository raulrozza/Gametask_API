{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier":[
      "error",
      {
        "singleQuote": true,
        "semi": true,
        "trailingComma": "all",
        "arrowParens": "avoid"
      }
    ],
    "camelcase": 0
  }
}
